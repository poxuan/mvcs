{
    "name": "callmecsx/mvcs",
    "description": "A fast file creator for Laravel.",
    "keywords": ["laravel", "mvcs"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/poxuan/mvcs/issues",
        "source": "https://github.com/poxuan/mvcs"
    },
    "authors": [
        {
            "name": "poxuan",
            "email": "958718201@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.5",
        "illuminate/validation": ">=5.5",
        "illuminate/filesystem": ">=5.5",
        "illuminate/console": ">=5.5",
        "phpoffice/phpspreadsheet": "^1.16"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": {
            "Callmecsx\\Mvcs\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        },
        "laravel": {
            "providers": [
                "Callmecsx\\Mvcs\\MvcsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
